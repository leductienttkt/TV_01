$('.list-user-certificate').html('<%= j render partial: "certificates/certificate", collection: current_user.certificates.newest %>');
<% if @status == 200 %>
  $.growl.notice({message: '<%= @message %>'});
<% else %>
  $.growl.error({message: '<%= @message %>'});
<% end %>
<% if current_user.certificates.blank? %>
  $('.user-certificate-hidden-toggle').addClass('hidden');
<% else %>
  $('.user-certificate-hidden-toggle').removeClass('hidden');
<% end %>
