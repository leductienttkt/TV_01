<% if @flash.status == 200 %>
  $.growl.notice({message: '<%= @flash.message %>'});
  $('.list-user-link').prepend(`<%= j render partial: "user_links/user_link",
    locals: {user_link: @user_link} %>`);
<% else %>
  $.growl.error({message: '<%= @flash.message %>'});
<% end %>
$('#formUserLinkModal').modal('hide');
if ($('.user-link-hidden-toggle').hasClass('hidden')) {
  $('.user-link-hidden-toggle').removeClass('hidden');
}
