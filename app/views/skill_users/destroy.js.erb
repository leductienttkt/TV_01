<% if @render.status == 200 %>
  $.growl.notice({message: '<%= @render.message %>'});
  $('.list-user-skill')
    .html('<%= j render partial: "skill_users/skill_user", collection: current_user.skill_users.includes(:skill) %>');
<% else %>
  $.growl.error({message: '<%= @render.message %>'});
<% end %>
<% if current_user.skill_users.blank? %>
  $('.user-skill-hidden-toggle').addClass('hidden');
<% else %>
  $('.user-skill-hidden-toggle').removeClass('hidden');
<% end %>
