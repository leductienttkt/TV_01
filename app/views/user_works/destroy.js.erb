$('.user-work').html('<%= j render "user_works/user_work", user: current_user %>');
<% if @render.status == 200 %>
  $.growl.notice({message: '<%= @render.message %>'});
<% else %>
  $.growl.error({message: '<%= @render.message %>'});
<% end %>
<% if current_user.user_works.blank? %>
  $('.user-work-hidden-toggle').addClass('hidden');
<% else %>
  $('.user-work-hidden-toggle').removeClass('hidden');
<% end %>
