<% if @render.status == 200 %>
  $.growl.notice({message: '<%= @render.message %>'});
  $('.user-language').html('<%= j render "user_languages/user_language", user: current_user %>');
<% else %>
  $.growl.error({message: '<%= @render.message %>'});
<% end %>
if ($('.user-language-hidden-toggle').hasClass('hidden')) {
  $('.user-language-hidden-toggle').removeClass('hidden');
}
